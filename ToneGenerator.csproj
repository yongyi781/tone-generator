  <ItemGroup>
    <Content Include="sine_wave.ico" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="NAudio.WinForms" Version="2.2.1" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
  </ItemGroup>

</Project>